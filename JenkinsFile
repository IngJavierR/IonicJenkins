pipeline {
   agent any
   environment {
      PATH='/usr/local/bin:/usr/bin:/bin'
   }
   stages {
      stage('NPM install') {
         steps {
            sh 'npm install'
         }
      }
      stage('Android Build') {
         steps {
            sh 'cordova telemetry on'
            sh 'ionic cordova build android'
         }
      }
   }
}
