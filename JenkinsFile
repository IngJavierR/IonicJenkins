pipeline {
   agent any
   environment {
      PATH='/usr/local/bin:/usr/bin:/bin'
   }
   stages {
      stage('NPM install') {
         steps {
            sh 'npm install'
         }
      }
      stage('Android Build') {
         steps {
            //sh 'ionic cordova build android'
            sh 'ls -l /var/lib/jenkins/android-sdk'
         }
      }
   }
}
